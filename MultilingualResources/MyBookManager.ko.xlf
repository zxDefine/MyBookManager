<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en-US" target-language="ko" original="MYBOOKMANAGER/STRINGS/EN-US/RESOURCES.RESW" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6920.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="MYBOOKMANAGER/STRINGS/EN-US/RESOURCES.RESW" datatype="resx">
        <trans-unit id="Btn_Back_Tip.Text" translate="yes" xml:space="preserve">
          <source>Back</source>
          <target state="translated">뒤</target>
        </trans-unit>
        <trans-unit id="Btn_ExportData_Tip.Text" translate="yes" xml:space="preserve">
          <source>Export the template or the collection library</source>
          <target state="translated">템플릿 또는 컬렉션 라이브러리 내보내기</target>
        </trans-unit>
        <trans-unit id="Btn_Help_Tip.Text" translate="yes" xml:space="preserve">
          <source>Help</source>
          <target state="translated">돕다</target>
        </trans-unit>
        <trans-unit id="Btn_ImportData_Tip.Text" translate="yes" xml:space="preserve">
          <source>Import the template or the collection library</source>
          <target state="translated">템플릿 또는 컬렉션 라이브러리 가져오기</target>
        </trans-unit>
        <trans-unit id="Btn_Language_Tip.Text" translate="yes" xml:space="preserve">
          <source>Change the app language</source>
          <target state="translated">앱 언어 변경</target>
        </trans-unit>
        <trans-unit id="ChangeLanguageDialogTitle" translate="yes" xml:space="preserve">
          <source>Do you want to change the app's language?</source>
          <target state="translated">앱의 언어를 변경하시겠습니까?</target>
        </trans-unit>
        <trans-unit id="ChangeLanguageDialogTxt" translate="yes" xml:space="preserve">
          <source>Changing the language will restart the app. Are you sure to restart?</source>
          <target state="translated">언어를 변경하면 앱이 다시 시작됩니다. 다시 시작하시겠습니까?</target>
        </trans-unit>
        <trans-unit id="ChangeLanguageDialog_No" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="translated">취소</target>
        </trans-unit>
        <trans-unit id="ChangeLanguageDialog_Yes" translate="yes" xml:space="preserve">
          <source>Sure</source>
          <target state="translated">예</target>
        </trans-unit>
        <trans-unit id="BtnFind.Content" translate="yes" xml:space="preserve">
          <source>Find</source>
          <target state="translated">찾다</target>
        </trans-unit>
        <trans-unit id="BtnFind_Tip.Text" translate="yes" xml:space="preserve">
          <source>Find all games and books.</source>
          <target state="translated">모든 게임과 책을 찾으십시오.</target>
        </trans-unit>
        <trans-unit id="BtnNewGame.Content" translate="yes" xml:space="preserve">
          <source>Add New Game</source>
          <target state="translated">새 게임 추가</target>
        </trans-unit>
        <trans-unit id="BtnNewGame_Tip.Text" translate="yes" xml:space="preserve">
          <source>Add a new game to the game collection.</source>
          <target state="translated">게임 컬렉션에 새 게임을 추가합니다.</target>
        </trans-unit>
        <trans-unit id="BtnNewBook.Content" translate="yes" xml:space="preserve">
          <source>Add New Book</source>
          <target state="translated">새 책 추가</target>
        </trans-unit>
        <trans-unit id="BtnNewBook_Tip.Text" translate="yes" xml:space="preserve">
          <source>Add a new book to the book collection.</source>
          <target state="translated">책 컬렉션에 새 책을 추가합니다.</target>
        </trans-unit>
        <trans-unit id="BtnViewGame.Content" translate="yes" xml:space="preserve">
          <source>View Game Collections</source>
          <target state="translated">게임 컬렉션 보기</target>
        </trans-unit>
        <trans-unit id="BtnViewGame_Tip.Text" translate="yes" xml:space="preserve">
          <source>View and edit a game info of the game collections.</source>
          <target state="translated">게임 컬렉션의 게임 정보를 보고 편집합니다.</target>
        </trans-unit>
        <trans-unit id="BtnViewBook.Content" translate="yes" xml:space="preserve">
          <source>View Book Collections</source>
          <target state="translated">도서 컬렉션 보기</target>
        </trans-unit>
        <trans-unit id="BtnViewBook_Tip.Text" translate="yes" xml:space="preserve">
          <source>View and edit a book info of the book collections.</source>
          <target state="translated">도서 컬렉션의 도서 정보를 보고 편집합니다.</target>
        </trans-unit>
        <trans-unit id="CommAddToText.Text" translate="yes" xml:space="preserve">
          <source>Add to:</source>
          <target state="translated">추가:</target>
        </trans-unit>
        <trans-unit id="Btn_Add_Book_Collection_Tip.Text" translate="yes" xml:space="preserve">
          <source>Create a new book collection.</source>
          <target state="translated">새 책 컬렉션을 만듭니다.</target>
        </trans-unit>
        <trans-unit id="Dialog_Create_New_Collection.PrimaryButtonText" translate="yes" xml:space="preserve">
          <source>OK</source>
          <target state="translated">좋아요</target>
        </trans-unit>
        <trans-unit id="Dialog_Create_New_Collection.SecondaryButtonText" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="translated">취소</target>
        </trans-unit>
        <trans-unit id="Dialog_Create_New_Collection.Title" translate="yes" xml:space="preserve">
          <source>Create a new collection</source>
          <target state="translated">새 컬렉션 만들기</target>
        </trans-unit>
        <trans-unit id="CreateNewCollectionLabelTxt_Book" translate="yes" xml:space="preserve">
          <source>Please input the new book collection name:</source>
          <target state="translated">새 책 컬렉션 이름을 입력하세요.</target>
        </trans-unit>
        <trans-unit id="CreateNewCollectionLabelTxt_Game" translate="yes" xml:space="preserve">
          <source>Please input the new game collection name:</source>
          <target state="translated">새 게임 컬렉션 이름을 입력하세요.</target>
        </trans-unit>
        <trans-unit id="Dialog_Create_New_Collection_Comment.Text" translate="yes" xml:space="preserve">
          <source>Only numbers, characters, and underscores.</source>
          <target state="translated">숫자, 문자 및 밑줄만 사용할 수 있습니다.</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>